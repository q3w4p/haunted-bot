# Haunted Bot Environment Variables
# Copy this file to .env and fill in your values

# Bot Token (already hardcoded in haunted.py, but can be moved here)
TOKEN=your_bot_token_here

# Database URL (Supabase PostgreSQL)
DATABASE_URL=postgresql://postgres.xxxxx:password@aws-1-us-east-1.pooler.supabase.com:6543/postgres

# Optional: API Keys
RIVAL_API=your_rival_api_key
EVICT_API=your_evict_api_key
